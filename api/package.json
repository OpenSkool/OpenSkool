{
  "name": "@os/api",
  "version": "0.0.1",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": "~16.17"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf .turbo/ coverage/ dist/ text.xml",
    "codegen": "tsx -r dotenv-flow/config scripts/codegen.ts",
    "dev": "NODE_ENV=development tsx watch --clear-screen=false --inspect -r dotenv-flow/config src/server.ts | FORCE_COLOR=1 pino-pretty",
    "lint": "eslint --color --ext .cjs,.mjs,.ts .",
    "postinstall": "prisma generate",
    "preview": "NODE_ENV=production node -r dotenv-flow/config dist/index.mjs | FORCE_COLOR=1 pino-pretty",
    "prisma:test": "DATABASE_URL='postgresql://openskool:openskool@localhost:5433/openskool-test' prisma",
    "start": "node dist/index.mjs",
    "test": "tsc --noEmit && vitest run --coverage --reporter=default --reporter=junit --outputFile=test.xml"
  },
  "dependencies": {
    "@casl/ability": "^6.2.0",
    "@casl/prisma": "~1.1.0",
    "@envelop/core": "^2.6.0",
    "@envelop/response-cache": "^3.2.0",
    "@faker-js/faker": "^7.5.0",
    "@fastify/awilix": "^3.0.1",
    "@fastify/cookie": "^8.2.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/env": "^4.1.0",
    "@fastify/session": "^10.0.0",
    "@graphql-yoga/common": "^2.12.12",
    "@graphql-yoga/node": "^2.13.13",
    "@hapi/boom": "^10.0.0",
    "@keycloak/keycloak-admin-client": "^19.0.2",
    "@mgcrea/fastify-graceful-exit": "^0.4.0",
    "@pothos/core": "^3.21.0",
    "@pothos/plugin-errors": "^3.7.0",
    "@pothos/plugin-prisma": "^3.35.4",
    "@prisma/client": "^4.4.0",
    "@sinclair/typebox": "0.24.42",
    "accept-language-parser": "^1.5.0",
    "awilix": "^7.0.3",
    "connect-redis": "^6.1.3",
    "cross-undici-fetch": "~0.4.14",
    "cuid": "^2.1.8",
    "fastify": "^4.6.0",
    "fastify-plugin": "^4.2.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "ioredis": "^5.2.2",
    "jose": "^4.9.3",
    "keyv": "^4.5.0",
    "ms": "3.0.0-canary.1",
    "openid-client": "^5.1.8",
    "p-limit": "^4.0.0",
    "pino": "^8.6.0",
    "prisma": "^4.4.0",
    "quick-lru": "^6.1.1",
    "read-pkg-up": "^9.1.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@fastify/type-provider-typebox": "^2.3.0",
    "@os/eslint-config": "workspace:*",
    "@os/tsconfig": "workspace:*",
    "@types/accept-language-parser": "^1.5.3",
    "@types/connect-redis": "^0.0.18",
    "@types/dotenv-flow": "^3.2.0",
    "@types/is-ci": "^3.0.0",
    "@types/prettier": "^2.7.0",
    "@vitest/coverage-c8": "^0.23.4",
    "c8": "^7.12.0",
    "dotenv-flow": "^3.2.0",
    "esbuild": "~0.15.8",
    "eslint": "^8.23.1",
    "graphql-tag": "^2.12.6",
    "is-ci": "^3.0.1",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.7.1",
    "rollup": "~2.79.0",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tsx": "^3.9.0",
    "type-fest": "^2.19.0",
    "typescript": "~4.8.3",
    "vite": "^3.1.3",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "0.23.4"
  },
  "prisma": {
    "seed": "tsx -r dotenv-flow/config prisma/seed.ts"
  }
}
