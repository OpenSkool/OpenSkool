{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": [".env.local", ".env.*.local"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**"]
    },
    "generate": {
      "cache": false
    },
    "@os/app#generate": {
      "dependsOn": ["@os/api#generate"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "lint": {
      "outputs": [".eslintcache"]
    },
    "preview": {
      "dependsOn": ["build"]
    },
    "test": {
      "outputs": ["test.xml"]
    }
  }
}
